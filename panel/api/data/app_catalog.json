      "id": "pihole",
      "name": "Pi-hole",
      "description": "Network-wide ad blocking via your own Linux hardware",
      "long_description": "Pi-hole is a DNS sinkhole that protects your devices from unwanted content, without installing any client-side software. Block ads and trackers on your entire network.",
      "category": "network",
      "image": "pihole/pihole:latest",
      "version": "latest",
      "website": "https://pi-hole.net",
      "documentation": "https://docs.pi-hole.net",
      "ports": [
        {"host": 8053, "container": 80, "protocol": "tcp", "description": "Web Interface"},
        {"host": 53, "container": 53, "protocol": "tcp", "description": "DNS TCP"},
        {"host": 53, "container": 53, "protocol": "udp", "description": "DNS UDP"}
      ],
      "volumes": [
        {"host": "/opt/pihole/etc-pihole", "container": "/etc/pihole", "description": "Pi-hole config"},
        {"host": "/opt/pihole/etc-dnsmasq.d", "container": "/etc/dnsmasq.d", "description": "DNS config"}
      ],
      "environment": [
        {"key": "TZ", "value": "Europe/Istanbul", "description": "Timezone"},
        {"key": "WEBPASSWORD", "value": "", "description": "Admin password (leave empty to generate)", "required": false}
      ],
      "capabilities": ["NET_ADMIN"],
      "restart_policy": "unless-stopped",
      "logo": "https://pi-hole.github.io/graphics/Vortex/Vortex_with_text.svg",
      "screenshots": [],
      "tags": ["dns", "ad-blocker", "privacy", "network"],
      "min_memory_mb": 128,
      "recommended_memory_mb": 256,
      "architecture": ["arm64", "arm/v7", "amd64"]
    },
